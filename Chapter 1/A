class MovieQuoteInfo{
   public static void main(String[] args) {
      System.out.println("Quote: Life is an endless series of trainwrecks with only brief commercial-like breaks of happiness.");
      System.out.println("The Movie: Deadpool ");
      System.out.println("The Actor: Ryan Reynolds ");
      System.out.println("Year: 2016 ");
      System.out.println("Director: Tim Miller ");
   }
}
